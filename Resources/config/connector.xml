    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
        
    <parameters>
        <parameter key="ihqs_contact.connector.email.class">Bundle\IHQS\ContactBundle\Connector\Email</parameter>
        <parameter key="ihqs_contact.connector.database.class">Bundle\IHQS\ContactBundle\Connector\Database</parameter>
    </parameters>

    <services>
